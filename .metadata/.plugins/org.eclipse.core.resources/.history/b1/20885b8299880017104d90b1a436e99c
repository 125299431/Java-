package 构造方法;

public class Test1 {
	public static void main(String[] args) {
		Person person = new Person(10, "张三");
		person.speak();
	}
}

class Person {
	private int age;
	private String name;
	
	Person(int a, String n) {
		// TODO Auto-generated constructor stub
		age = a;
		name = n;
	}
	
	public void speak() {
		System.out.println("name = " + name + " age = " + age);
	}
}
