/*
 *比较两个对象 重写equals 
 */

class Person11 {
	int age;
	
	public boolean equals(Object object){
		if (this == object) {
			return true;
		}
		
		if (!(object instanceof Person11)){
			return false;
		}
		
		Person11 person = (Person11)object;
		return person.age == this.age;
	}
}
